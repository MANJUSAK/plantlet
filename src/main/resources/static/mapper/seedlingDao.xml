            </if>
            <if test="spec != null and spec != ''">
                AND spec= #{specMin}
            </if>
            <if test="specMin != null and specMin != ''">
                AND spec_min &gt;= #{spec}
            </if>
            <if test="specMax != null and specMax != ''">
                AND spec_max &lt;= #{specMax}
            </if>
            <if test="year != null and year != ''">
                AND `year` = #{year}
            </if>
        </where>
        LIMIT #{num},20;
