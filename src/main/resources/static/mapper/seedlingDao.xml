        <result property="Nov" column="November"/>
        <result property="Dec" column="December"/>
    </resultMap>
    <!--苗木造价统计年月查询结果映射 end-->
    <!--苗木造价统计年月查询sql start-->
    <select id="querySeedlingOfferStatisticsDetailDao"
            parameterType="com.goodsoft.plantlet.util.result.SeedlingOfferParam"
            resultMap="seedlingOfferStatisticsDetailMap">
        SELECT `seedling_name`,`spec`,`spec_min`,`spec_max`, `comment`,`year`,`January`,`February`,`March`,
        `April`,`May`,`June`,`July`,`August`,`September`,`October`,`November`,`December`
        FROM `gs_seedling_offer_view`
        <where>
            <if test="keyWord != null and keyWord != ''">
